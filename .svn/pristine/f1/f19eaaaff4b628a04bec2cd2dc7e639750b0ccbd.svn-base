/* pages/home/home.wxss */
.item_ul{
  display: -webkit-flex;
  display: flex;
  flex-flow: row wrap;
  align-items: center;
  justify-content: space-around;
  width:100%;
  padding: 30rpx 0 5rpx;
}
.item_ul_li{
  width: 20%;
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
  font-size: 28rpx;
  padding-bottom: 25rpx;
}
.bannerCarImg{
  width:90rpx;
  height: 90rpx;
  align-self: center;
}
.HomeHeaderBanner swiper{
  height: 50vw;
}
.autoshow_ul{
  display: flex;
  flex-flow: column wrap;
  justify-content: flex-start;
}
.autoshow_li{
  display: flex;
  flex-flow: column wrap;
  align-items: flex-start;
  align-content: flex-start;
}